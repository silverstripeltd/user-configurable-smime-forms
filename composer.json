{
    "name": "silverstripe/smime-forms",
    "description": "A module for extending user forms, so that form submissions can be emailed with S/MIME encryption.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "S/MIME",
        "encryption"
    ],
    "authors": [
        {
            "name": "SilverStripe",
            "homepage": "http://silverstripe.com"
        }
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/vendor-plugin": "^1",
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "silverstripe/userforms": "^5.13",
        "silverstripe/silverstripe-smime": "dev-feature/tidy-up"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.0",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "slevomat/coding-standard": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\SmimeForms\\": "src/",
            "SilverStripe\\SmimeForms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/silverstripeltd/silverstripe-smime.git"
        }
    ]
}
